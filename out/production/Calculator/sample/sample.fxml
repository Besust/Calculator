<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="240.0" prefWidth="400.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
    <children>
        <TextField fx:id="textField" prefHeight="40.0" prefWidth="400.0"/>
        <HBox layoutY="40.0">
            <children>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="7"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="8"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="9"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="/"/>
            </children>
        </HBox>
        <HBox layoutY="80.0" AnchorPane.topAnchor="80.0">
            <children>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="4"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="5"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="6"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="*"/>
            </children>
        </HBox>
        <HBox layoutY="120.0" AnchorPane.topAnchor="120.0">
            <children>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="1"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="2"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="3"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="-"/>
            </children>
        </HBox>
        <HBox layoutY="160.0" AnchorPane.topAnchor="160.0">
            <children>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="0"/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="."/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="="/>
                <Button mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="+"/>
            </children>
        </HBox>
        <Button layoutY="200.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="400.0" text="C"
                textAlignment="CENTER"/>

    </children>
</AnchorPane>
